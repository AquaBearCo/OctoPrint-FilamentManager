---
"extends":
    - "airbnb"
"rules":
    "indent": ["error", 4]
    "max-len": ["error", 120, {
      ignoreUrls: true,
      ignoreComments: false,
      ignoreRegExpLiterals: true,
      ignoreStrings: true,
      ignoreTemplateLiterals: true,
    }]
"globals":
    "_": false
    "$": false
    "ko": false
    "gettext": false
    "showConfirmationDialog": false
    "ItemListHelper": false
    "PNotify": false
    "Utils": false
    "FilamentManager": false
    "OCTOPRINT_VIEWMODELS": false
